{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "env": [
        "RUSTUP_HOME",
        "CARGO_HOME"
      ],
      "outputs": [
        "dist/apps/app/**",
        "src-tauri/target/**"
      ],
      "cache": true
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ],
      "outputs": [],
      "cache": true
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ],
      "outputs": [],
      "cache": true
    },
    "dev": {
      "cache": false,
      "env": [
        "RUSTUP_HOME",
        "CARGO_HOME"
      ],
      "persistent": true,
      "dependsOn": [
        "^dev"
      ]
    },
    "test": {
      "dependsOn": [
        "^test"
      ],
      "outputs": [],
      "cache": true
    }
  }
}